{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "flutter build windows",
			"type": "shell",
			"command": "flutter",
			"args": [
				"build",
				"windows"
			],
			"problemMatcher": [
				"$dart"
			],
			"group": "build"
		},
		{
			"label": "flutter run emulator",
			"type": "shell",
			"command": "flutter",
			"args": [
				"run"
			],
			"problemMatcher": [
				"$dart"
			],
			"group": "build"
		}
	]
}